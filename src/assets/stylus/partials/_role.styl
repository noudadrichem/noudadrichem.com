#roles
  width:70%
  margin:40px auto
  height:auto
  &:after
    content:''
    display:block
    clear:both
  .list
    float:left
    width:30%
    margin:0 5% 0 0
    &:last-child
      margin:0 0 0 0
    h1
      color:darkblue
      text-transform:uppercase
      font-size:1em
    hr
      border-color:darkblue
      border-size:2px
      width:40%
      margin:0 0 8px 0
    ul
      list-style:none
      li
        color:grey
        font-weight:300
